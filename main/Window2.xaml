<Window x:Class="Prueba.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prueba"
        mc:Ignorable="d"
        Title="Window2" Height="450" Width="800">
    <Grid>
        <Rectangle HorizontalAlignment="Center" Height="368" Margin="0,66,0,0" Stroke="Black" VerticalAlignment="Top" Width="800" Fill="Black"/>
        <Image Margin="10,271,521,10" Source="/Image3.png" Stretch="Fill"/>
        <Image Margin="384,100,74,168" Source="/Image4.png" Stretch="Fill"/>
        <Image Margin="10,98,439,168" Source="/Image5.png" Stretch="Fill"/>
        <Image Margin="0,294,122,10" Source="/Image6.png" Stretch="Fill" HorizontalAlignment="Right" Width="294"/>
        <Grid x:Name="header1" HorizontalAlignment="Center" Height="66" VerticalAlignment="Top" Width="800" Background="White">
            <Rectangle HorizontalAlignment="Left" Height="46" Margin="668,0,0,0"  VerticalAlignment="Center" Width="114" MouseDown="Rectangle_MouseDown">
                <Rectangle.Fill>
                    <ImageBrush ImageSource="/use-removebg-preview.png" Stretch="Uniform" TileMode="None"/>
                </Rectangle.Fill>
            </Rectangle>
            <Rectangle HorizontalAlignment="Left" Height="46"  VerticalAlignment="Center" Width="120">
                <Rectangle.Fill>
                    <ImageBrush ImageSource="/Logo-Tesla-removebg-preview.png" Stretch="Uniform"/>
                </Rectangle.Fill>
            </Rectangle>
            <Rectangle HorizontalAlignment="Left" Height="66" Margin="125,0,0,0"  VerticalAlignment="Center" Width="138" MouseDown="Rectangle_MouseDown_1">
                <Rectangle.Fill>
                    <ImageBrush ImageSource="/coche.png" Stretch="Uniform"/>
                </Rectangle.Fill>
            </Rectangle>
        </Grid>

    </Grid>
</Window>
